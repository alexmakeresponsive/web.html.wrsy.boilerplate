.s-grid-container
  margin 0 auto

  @media screen and (max-width: 575px)
    max-width 100%
  @media screen and (min-width: 576px)
    max-width 540px
  @media screen and (min-width: 768px)
    max-width 720px
  @media screen and (min-width: 992px)
    max-width 960px
  @media screen and (min-width: 1200px)
    max-width 1160px
